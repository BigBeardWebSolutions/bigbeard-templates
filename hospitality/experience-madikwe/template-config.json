  "image_slots": [
    {
      "selector": ".hero-bg img",
      "property": "src",
      "path": "images/hero-bg.jpg",
      "keywords": ["hero", "safari", "landscape", "sunset", "nature", "background"],
      "aspect_ratio": "16:9",
      "min_width": 1920
    },
    {
      "selector": ".hero-overlay img",
      "property": "src",
      "path": "images/hero-overlay.png",
      "keywords": ["overlay", "gradient", "decorative"],
      "aspect_ratio": "16:9",
      "min_width": 1920
    },
    {
      "selector": ".header-logo img",
      "property": "src",
