  "sections": {
    "hero": {
      "required": true,
      "customizable": true,
      "description": "Full-viewport hero section with background image, radial gradient overlay, headline, description, and primary CTA button"
    },
    "about": {
      "required": true,
      "customizable": true,
      "description": "Two-column about section with text content on the left and large wildlife image on the right"
    },
    "stats": {
      "required": true,
      "customizable": true,
      "description": "Five icon-based stat/experience items in a horizontal grid with circular icon backgrounds and labels"
    },
    "lodges": {
      "required": true,
      "customizable": true,
      "description": "3x3 lodge gallery grid with image cards, hover zoom effect, gradient overlay with lodge name, and CTA button"
    },
    "directions": {
      "required": false,
      "customizable": true,
      "description": "Centered getting-here section with heading, description, and secondary CTA button"
    },
    "footer": {
      "required": true,
      "customizable": true,
      "description": "Golden-toned footer with logo, contact info, navigation columns, social media icons, legal links, and copyright"
    }
  },
