  "sections": {
    "top-bar": {
      "required": true,
      "customizable": true,
      "description": "Blue top bar with phone and email contact info"
    },
    "header": {
      "required": true,
      "customizable": true,
      "description": "Sticky header with logo, navigation links, and Contact Us CTA button"
    },
    "hero": {
      "required": true,
      "customizable": true,
      "description": "Full-width banner slider with 3 slides, gradient overlay, headline, subtitle, and CTA button"
    },
    "about": {
      "required": true,
      "customizable": true,
      "description": "Two-column section with image on left and About Us text with Read More CTA on right"
    },
    "focus": {
      "required": true,
      "customizable": true,
      "description": "Three-column focus area cards with circular icons, titles, dividers, and descriptions"
    },
    "what-we-do": {
      "required": true,
      "customizable": true,
      "description": "Full-width background image section with dark overlay, label, heading, description, and CTA"
    },
    "trustees": {
      "required": true,
      "customizable": true,
      "description": "Two-column trustee profile cards with arch-topped photos, names, roles, bios, and read more links"
    },
    "apply-cta": {
      "required": true,
      "customizable": true,
      "description": "Full-width background image CTA section with title, subtitle, and download button"
    },
    "contact": {
      "required": true,
      "customizable": true,
      "description": "Two-column contact section with info/social on left and form with name, email, message, terms checkbox, marketing opt-in on right"
    },
    "speak-out": {
      "required": false,
      "customizable": true,
      "description": "Blue banner section for anti-corruption or transparency messaging with CTA"
    },
    "footer": {
      "required": true,
      "customizable": true,
      "description": "Four-column footer with quick links, address, contact info with social icons, legal links, and green copyright bar"
    }
  },
