      "min_width": 86
    },
    {
      "selector": ".focus__icon-img:nth-of-type(3)",
      "property": "src",
      "path": "images/focus-icon-3.png",
      "keywords": ["icon", "food", "security", "nutrition"],
      "aspect_ratio": "1:1",
      "min_width": 86
    },
    {
      "selector": ".what-we-do__bg",
      "property": "src",
      "path": "images/what-we-do-bg.jpg",
      "keywords": ["landscape", "african", "savanna", "community"],
      "aspect_ratio": "16:9",
      "min_width": 1920
    },
    {
      "selector": ".trustee-card__image:nth-child(1)",
      "property": "src",
      "path": "images/trustee-1.png",
      "keywords": ["trustee", "portrait", "person", "leader"],
      "aspect_ratio": "auto",
      "min_width": 379
    },
    {
      "selector": ".trustee-card__image:nth-child(2)",
