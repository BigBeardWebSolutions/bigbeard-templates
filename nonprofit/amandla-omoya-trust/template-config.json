  "image_slots": [
    {
      "selector": ".hero__slide-image:nth-child(1)",
      "property": "src",
      "path": "images/hero-bg.jpg",
      "keywords": ["hero", "community", "sunset", "people"],
      "aspect_ratio": "16:9",
      "min_width": 1920
    },
    {
      "selector": ".hero__slide-image:nth-child(2)",
      "property": "src",
      "path": "images/hero-slide-2.jpg",
      "keywords": ["hero", "community", "development"],
      "aspect_ratio": "16:9",
      "min_width": 1920
    },
    {
      "selector": ".hero__slide-image:nth-child(3)",
      "property": "src",
      "path": "images/hero-slide-3.jpg",
      "keywords": ["hero", "renewable energy", "solar"],
      "aspect_ratio": "16:9",
      "min_width": 1920
    },
    {
      "selector": ".header__logo img",
      "property": "src",
      "path": "images/logo.png",
      "keywords": ["logo", "brand", "trust"],
      "aspect_ratio": "auto",
      "min_width": 124
    },
    {
      "selector": ".about__image",
      "property": "src",
      "path": "images/about-image.jpg",
      "keywords": ["about", "solar", "renewable energy", "plant"],
      "aspect_ratio": "3:2",
      "min_width": 795
    },
    {
      "selector": ".focus__icon-img:nth-of-type(1)",
      "property": "src",
      "path": "images/focus-icon-1.png",
      "keywords": ["icon", "enterprise", "market", "development"],
      "aspect_ratio": "1:1",
      "min_width": 86
    },
    {
      "selector": ".focus__icon-img:nth-of-type(2)",
      "property": "src",
      "path": "images/focus-icon-2.png",
