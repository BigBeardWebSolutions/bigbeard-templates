  "sections": {
    "topbar": {
      "required": true,
      "customizable": true,
      "description": "Green top bar with phone number and email contact information"
    },
    "header": {
      "required": true,
      "customizable": true,
      "description": "Sticky header with logo, navigation links (Home, About Us, Opportunities dropdown, Programmes, Partnership, News), and Contact Us CTA button"
    },
    "hero": {
      "required": true,
      "customizable": true,
      "description": "Full-width hero slider with 3 slides, rounded-corner right edge, gradient overlay, headline, subtitle, CTA button, and dot navigation"
    },
    "about": {
      "required": true,
      "customizable": true,
      "description": "Two-column about section with rounded image on the left and text content (label, title, description, CTA) on the right"
    },
    "focus": {
      "required": true,
      "customizable": true,
      "description": "Three-column focus areas section with circular icons, titles, decorative lines, and descriptions for enterprise development, youth education, and food security"
    },
    "impact": {
      "required": true,
      "customizable": true,
      "description": "Full-width impact section with landscape background image, dark gradient overlay, label, title, description, and CTA button"
    },
    "trustees": {
      "required": true,
      "customizable": true,
      "description": "Two-column trustee profiles with arch-shaped image backgrounds (blue gradient), trustee photo, name, role, LinkedIn icon, bio, and read more link"
    },
    "apply-banner": {
      "required": true,
      "customizable": true,
      "description": "Full-width CTA banner with background image, dark overlay, title, description, and download button"
    },
    "contact": {
      "required": true,
      "customizable": true,
      "description": "Two-column contact section with contact info (email, phone, address, social media) on the left and contact form (name, email, message, terms checkbox, marketing opt-in, submit) on the right"
    },
    "speak-out": {
      "required": false,
      "customizable": true,
      "description": "Speak out section with full-width image and a dark overlay banner for anti-corruption reporting with title, description, and CTA"
    },
    "footer": {
      "required": true,
      "customizable": true,
      "description": "Green footer with four columns: navigation links, physical address, phone/email/social media, and legal links. Blue bottom bar with copyright text"
    }
  },
