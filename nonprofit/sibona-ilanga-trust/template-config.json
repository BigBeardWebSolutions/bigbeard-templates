  "sections": {
    "hero": {
      "required": true,
      "customizable": true,
      "description": "Hero banner slider with three rotating images, dark gradient overlay, headline, sub-headline, CTA button, and slide indicator dots"
    },
    "about": {
      "required": true,
      "customizable": true,
      "description": "About us section with rounded-corner image on the left, uppercase label, title, description paragraph, and Read More CTA on the right"
    },
    "focus-areas": {
      "required": true,
      "customizable": true,
      "description": "Two focus area cards with circular orange icon backgrounds, titles, decorative lines, and description text"
    },
    "what-we-do": {
      "required": true,
      "customizable": true,
      "description": "Full-width section with background image, dark overlay, left-aligned gradient, uppercase label, title, description, and CTA"
    },
    "community": {
      "required": true,
      "customizable": true,
      "description": "Community engagement section with background image, dark overlay, centered title, description, and Download CTA"
    },
    "trustees": {
      "required": true,
      "customizable": true,
      "description": "Trustee profiles section with photo on gradient arch background, name in primary blue, role label, biography text, and read more link"
    },
    "footer": {
      "required": true,
      "customizable": true,
      "description": "Dark blue footer with four columns (quick links, address, contact/social, legal), horizontal divider, and orange bottom bar with copyright"
    }
  },
  "css_variables": {
