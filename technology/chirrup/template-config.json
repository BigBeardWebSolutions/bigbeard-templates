  "image_slots": [
    {
      "selector": ".navbar__logo img",
      "property": "src",
      "path": "images/logo.png",
      "keywords": [
        "logo",
        "brand",
        "navigation"
      ],
      "aspect_ratio": "auto",
      "min_width": 200
    },
    {
      "selector": ".hero__background img",
      "property": "src",
      "path": "images/hero-bg.jpg",
      "keywords": [
        "hero",
        "background",
        "landscape",
        "nature"
      ],
      "aspect_ratio": "16:9",
      "min_width": 1920
    },
    {
      "selector": ".hero__logo img",
      "property": "src",
      "path": "images/hero-wordmark.png",
      "keywords": [
